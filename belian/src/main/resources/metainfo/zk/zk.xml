<zk>
    <config-name>belian-config</config-name>
	<richlet>
	    <richlet-name>Home</richlet-name>
	    <richlet-class>com.kratonsolution.belian.ui.Desktop</richlet-class>
	</richlet>
	<richlet-mapping>
	    <richlet-name>Home</richlet-name>
	    <url-pattern>/home</url-pattern>
	</richlet-mapping>
	<richlet>
	    <richlet-name>Failur</richlet-name>
	    <richlet-class>com.kratonsolution.belian.ui.Failur</richlet-class>
	</richlet>
	<richlet-mapping>
	    <richlet-name>Failur</richlet-name>
	    <url-pattern>/error</url-pattern>
	</richlet-mapping>
	<session-config>
 		<timer-keep-alive>true</timer-keep-alive>
	</session-config>
</zk>